{
  "name": "@hitechline/next-mdx",
  "version": "0.0.1",
  "license": "AGPL-3.0",
  "main": "dist/index.js",
  "module": "dist/next-mdx.esm.js",
  "types": "typings/index.d.ts",
  "scripts": {
    "fix": "yarn lint:fix && yarn prettier:fix",
    "clean": "rimraf dist typings",
    "prepublishOnly": "yarn build",
    "release": "yarn node-env-run --exec \"semantic-release --ci false\"",
    "release:check": "yarn node-env-run --exec \"semantic-release\"",
    "test": "jest --passWithNoTests",
    "test:watch": "yarn test --watchAll",
    "build": "tsdx build --transpileOnly",
    "prebuild": "yarn clean",
    "lint": "eslint ./ --ext js,jsx,ts,tsx",
    "lint:fix": "eslint ./ --ext js,jsx,ts,tsx --fix",
    "prettier": "prettier --check ./**/*.{js,jsx,ts,tsx}",
    "prettier:fix": "prettier --write ./**/*.{js,jsx,ts,tsx}"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hitechline/next-mdx.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "directories": {
    "lib": "src",
    "test": "__tests__"
  },
  "files": [
    "dist",
    "typings"
  ],
  "keywords": [
    "react",
    "next",
    "mdx",
    "render"
  ],
  "peerDependencies": {
    "react": "^16.8.0",
    "react-dom": "^16.8.0"
  },
  "dependencies": {
    "@babel/core": "^7.12.17",
    "@babel/plugin-transform-react-jsx": "^7.12.17",
    "@babel/preset-env": "^7.12.17",
    "@babel/preset-react": "^7.12.13",
